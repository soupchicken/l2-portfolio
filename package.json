{
  "name": "l2-portfolio",
  "private": false,
  "version": "0.0.1",
  "description": "L2's very own portfolio app",
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run clean && export NODE_ENV=production && mkdir build && webpack",
    "local": "export NODE_ENV=local && webpack && babel-node server.js",
    "production": "npm run build && pm2 kill && pm2 start build/server.js -i 1 --name='portfolio'",
    "build-win": "npm run clean && set NODE_ENV=production&& mkdir build && webpack",
    "local-win": "set NODE_ENV=local&& webpack && babel-node server.js",
    "production-win": "npm run build-win && pm2 kill &&  pm2 start build/server.js -i 1 --name='portfolio'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soupchicken/l2-portfolio"
  },
  "keywords": [
    "react",
    "portfolio",
    "l2"
  ],
  "author": "Lucas Lemos",
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.6.1",
    "mocha": "^5.0.0",
    "react-hot-loader": "^3.1.3",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "backbone": "^1.3.3",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "create-react-class": "^15.6.3",
    "css-loader": "^0.28.9",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "jquery": "^3.5.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.5",
    "mobile-detect": "^1.4.1",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "pm2": "^2.10.1",
    "prop-types": "^15.6.0",
    "react": "^15.0.0",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "velocity-animate": "^1.5.1",
    "webpack-node-externals": "^1.6.0"
  }
}
